{
  "name": "Foosball Tracker",
  "version": "0.8.4",
  "private": true,
  "type": "module",
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.85.5",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "lucide-react": "^0.487.0",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.55.0",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.85.5",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.10.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.8",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.17",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.0.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "postcss": "^8.4.35",
    "prettier": "^3.6.2",
    "supabase": "^2.39.2",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "vite": "^5.4.19",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:ci": "vitest run --reporter=verbose --coverage",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives --max-warnings 200",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "ci:quality-gates": "npm run lint && npm run format:check && npm run type-check && npm run test:run && npm run build",
    "ci:security-audit": "npm audit --audit-level=high",
    "ci:performance-budget": "node scripts/performance-budget-validator.cjs",
    "ci:preview-test": "node scripts/preview-environment-tests.js",
    "ci:bundle-analysis": "npm run build && echo 'Build completed successfully - bundle analysis done'",
    "ci:health-check": "node scripts/deployment-rollback.js health",
    "ci:rollback-monitor": "node scripts/deployment-rollback.js monitor",
    "ci:rollback-force": "node scripts/deployment-rollback.js rollback",
    "ci:full": "npm run ci:quality-gates && npm run ci:security-audit && npm run ci:performance-budget",
    "deploy:preview": "vercel --confirm",
    "deploy:production": "vercel --prod --confirm",
    "rollback:auto": "npm run ci:rollback-monitor",
    "rollback:manual": "npm run ci:rollback-force",
    "migrate:data": "node scripts/run-migration.js",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
